  "terminal.integrated.env.windows"       : {},
  "window.title"                          : "${dirty}$,${folderName}{separator}${remoteName},{activeEditorShort}${separator}${rootName}${separator}${profileName}${separator}${appName}",
  "workbench.editor.tabCloseButton"       : "left",
  "workbench.fontAliasing"                : "antialiased",
  "workbench.list.smoothScrolling"        : true,
  "workbench.startupEditor"               : "newUntitledFile",
  "workbench.tree.expandMode"             : "singleClick",
  "workbench.tree.indent"                 : 10,
  "workbench.iconTheme"                   : "material-icon-theme",
  "workbench.tips.True"                   : false,
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.activityBar.location"        : "bottom",
  "workbench.editor.closeOnFileDelete"    : true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.True"           : true,
  "workbench.editor.limit.perEditorGroup" : true,
  "workbench.editor.limit.value"          : 5,
  //"markdown-image.local.referencePath": : ${index},
  "image.base.fileNameFormat" : "\\*"
